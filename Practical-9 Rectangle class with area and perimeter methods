public class Rectangle class with area and perimeter methods
{ 
 	double width = 1; 
 	double height=1; 
 	Rectangle() 
 	{ 
 	} 
 	Rectangle(double w , double h) 
 	{ 
 	 	width=w;  	 	height=h; 
 	} 
 	double getArea() 
 	{ 
 	 	return width * height; 
 	} 
 	double getPerimeter() 
 	{ 
 	 	return 2 * (width + height); 
 	} 
 	public static void main(String args[]) 
 	{ 
 	 	Rectangle r1 = new Rectangle(); 
 	 	Rectangle r2 = new Rectangle(5,3); 
 	 	System.out.println("Rectangle 1 Area: " + r1.getArea()); 
 	 	System.out.println("Rectangle 1 Perimeter:"+r2.getPerimeter()); 
 	} 
} 
