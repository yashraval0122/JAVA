import java.util.Scanner;

class BankAccount
{
	String accountId;
	String accountHolderName;
	double balance;

	void assignValues(String id, String name, double bal)
	{
		accountId = id;
		accountHolderName = name;
		balance = bal;
	}
	
	void displayValues()
	{
		System.out.println("Account ID: " + accountId);
		System.out.println("Balance: " + balance);
	}
	static void searchAccount(BankAccount[] acc, String searchId)
	{
		Boolean found = false;
		for (BankAccount a : acc)
		{
			if(a.accountId.equals(searchId))
			{
				a.displayValues();
				found = true;
				break;
			}
		}
			if (!found)
			{
				System.out.println("Account not found!");
			}
	}
		public static void main(String []args)
		{
			Scanner sc = new Scanner(System.in);
		
			BankAccount[] accounts = new BankAccount[5];
			  
			accounts[0] = new BankAccount();
			accounts[0].assignValues("A101","Yash R.", 5000);

			accounts[1] = new BankAccount();
			accounts[1].assignValues("A102","Yash S.", 7000);

			accounts[2] = new BankAccount();
			accounts[2].assignValues("A103","Raiyan S.", 9000);
	
			accounts[3] = new BankAccount();
			accounts[3].assignValues("A104","Rishi", 6000);

			accounts[4] = new BankAccount();
			accounts[4].assignValues("A105","Abhishek", 8000);

			System.out.println("Enter Account ID to search:");
	
			String id = sc.next();
			
			searchAccount(accounts, id);
		}
}
